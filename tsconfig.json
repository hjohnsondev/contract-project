{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/api/preview.js",
    "pages/api/exit-preview.ts"
, "components/PreviewBanner.jsx", "components/SlugPage.jsx", "components/PreviewBanner.jsx", "components/CategoriesMenu.tsx", "components/Header.tsx", "components/Banner.js"  ],
  "exclude": [
    "node_modules"
  ]
}
